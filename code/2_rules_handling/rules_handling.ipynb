    "from plotly.subplots import make_subplots\n",
    "import plotly.graph_objects as go\n",
    "\n",
    "# Custom colorscale: 0 (low) = light blue, 1 (high) = darker purple\n",
    "colorscale = [[0, 'lightblue'], [1, 'mediumorchid']]\n",
    "\n",
    "# Color values mapped to rejection probability\n",
    "age_colors = age_rejection['REJECTED']\n",
    "bmi_colors = bmi_rejection['REJECTED']\n",
    "\n",
    "fig = make_subplots(rows=1, cols=2, subplot_titles=(\"Rejection Probability by Age\", \"Rejection Probability by BMI\"))\n",
    "\n",
    "# Age plot\n",
    "fig.add_trace(go.Bar(\n",
    "    x=age_rejection['AGE_BIN'].astype(str),\n",
    "    y=age_rejection['REJECTED'],\n",
    "    text=[f\"{v:.2f}\" for v in age_rejection['REJECTED']],\n",
    "    textposition='outside',\n",
    "    marker=dict(\n",
    "        color=age_colors,\n",
    "        colorscale=colorscale,\n",
    "        cmin=0,\n",
    "        cmax=1,\n",
    "        colorbar=dict(title=\"P(rejected)\")\n",
    "    )\n",
    "), row=1, col=1)\n",
    "\n",
    "# BMI plot\n",
    "fig.add_trace(go.Bar(\n",
    "    x=bmi_rejection['BMI_BIN'].astype(str),\n",
    "    y=bmi_rejection['REJECTED'],\n",
    "    text=[f\"{v:.2f}\" for v in bmi_rejection['REJECTED']],\n",
    "    textposition='outside',\n",
    "    marker=dict(\n",
    "        color=bmi_colors,\n",
    "        colorscale=colorscale,\n",
    "        cmin=0,\n",
    "        cmax=1,\n",
    "        colorbar=dict(title=\"P(rejected)\")\n",
    "    )\n",
    "), row=1, col=2)\n",
    "\n",
    "# Update layout with axis labels and formatting\n",
    "fig.update_layout(\n",
    "    height=500, width=1000,\n",
    "    showlegend=False,\n",
    "    yaxis=dict(range=[0, 1], title=\"Probability\"),\n",
    "    yaxis2=dict(range=[0, 1], title=\"Probability\"),\n",
    "    xaxis=dict(title=\"Age\"),\n",
    "    xaxis2=dict(title=\"Body Mass Index\"),\n",
    "    margin=dict(t=50, l=50, r=50, b=50)\n",
    ")\n",
    "\n",
    "fig.show()\n"
   ]
